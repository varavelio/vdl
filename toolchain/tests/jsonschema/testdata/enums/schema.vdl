// Tests string and integer enums

""" Status of an order """
enum OrderStatus {
  Pending = "pending"
  Processing = "processing"
  Shipped = "shipped"
  Delivered = "delivered"
  Cancelled = "cancelled"
}

""" HTTP status codes """
enum HttpStatus {
  Ok = 200
  Created = 201
  BadRequest = 400
  NotFound = 404
  ServerError = 500
}

""" Priority levels """
enum Priority {
  Low = 1
  Medium = 2
  High = 3
  Critical = 4
}

type Order {
  id: string
  status: OrderStatus
  priority: Priority
}

rpc OrderService {
  proc GetOrder {
    input {
      id: string
    }

    output {
      order: Order
      httpStatus: HttpStatus
    }
  }

  proc UpdateStatus {
    input {
      id: string
      status: OrderStatus
    }

    output {
      success: bool
    }
  }
}
