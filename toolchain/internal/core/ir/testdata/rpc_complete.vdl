type PaginationParams {
  page int
  limit int
}

type PaginatedResponse {
  totalItems int
  totalPages int
}

type User {
  id string
  name string
  email string
}

type UserQuery {
  ...PaginationParams
  filterByName? string
}

type UserListResult {
  ...PaginatedResponse
  users User[]
}

enum UserEventAction {
  Created = "created"
  Updated = "updated"
}

const maxPageSize = 100

const queryDefaults = {
  page 1
  limit maxPageSize
}
