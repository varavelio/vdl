"""
Edge case schema for IR conversion.
"""

type EmptyType {
}

type SingleField {
  only string
}

type DeepInline {
  level1 {
    level2 {
      level3 {
        value string
      }
    }
  }
}

type OptionalComplex {
  optMatrix? int[][]
  optMapMap? map[map[string]]
  optObject? {
    innerOpt? string
    innerReq int
  }
}

type SpreadBase {
  baseField string
}

type InlineWithSpread {
  ...SpreadBase
  inlineObj {
    nested string
  }
}

enum Status {
  On
  Off
}

type EnumsEverywhere {
  status Status
  statusList Status[]
  statusMatrix Status[][]
  statusMap map[Status]
}

const zeroInt = 0
const emptyString = ""
const matrix = [[1 2] [3 4]]
const nestedObj = {
  a {
    b {
      c true
    }
  }
}
