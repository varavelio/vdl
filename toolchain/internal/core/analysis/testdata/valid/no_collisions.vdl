// Valid schema with no synthetic name collisions
// Enums, types, and RPCs coexist peacefully

enum Color {
  Red
  Blue
  Green
}

enum Status {
  Active
  Inactive
  Pending
}

type User {
  id: string
  name: string
  color: Color
  status: Status
}

type Product {
  id: string
  title: string
}

rpc Users {
  proc GetUser {
    input {
      id: string
    }

    output {
      user: User
    }
  }

  proc CreateUser {
    input {
      name: string
    }

    output {
      user: User
    }
  }

  stream WatchUsers {
    input {
      filter: string
    }

    output {
      user: User
    }
  }
}

rpc Products {
  proc ListProducts {
    input {
      limit: int
    }

    output {
      products: Product[]
    }
  }
}
