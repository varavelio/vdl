// @expect-no-errors
// @assert: has-type Catalog

type Audit {
  createdAt datetime
  updatedAt datetime
}

type Flags {
  active bool
  archived bool
}

type Catalog {
  ...Audit
  sections {
    id string
    title string
    flags Flags
    entries {
      id string
      metadata {
        labels string[]
        scores int[][]
      }
      variants {
        code string
        price float
      }[]
    }[]
  }[]
  byId map[string]
}
