// Various valid enum patterns

// String enum with implicit values
enum Status {
	Active
	Inactive
	Pending
}

// String enum with explicit values
enum HttpMethod {
	Get = "GET"
	Post = "POST"
	Put = "PUT"
	Delete = "DELETE"
}

// Integer enum
enum Priority {
	Low = 1
	Medium = 5
	High = 10
	Critical = 100
}

// Enum used as field type
type Task {
	id: string
	status: Status
	priority: Priority
	method: HttpMethod
}
