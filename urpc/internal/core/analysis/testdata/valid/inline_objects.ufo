// Inline object types

type WithInlineObject {
	name: string
	address: {
		street: string
		city: string
		country: string
	}
}

type NestedInlineObjects {
	data: {
		user: {
			profile: {
				bio: string
				avatar: string
			}
			settings: {
				theme: string
				notifications: bool
			}
		}
	}
}

type InlineWithOptional {
	metadata?: {
		createdAt: datetime
		updatedAt?: datetime
	}
}

type InlineArray {
	items: {
		id: string
		name: string
	}[]
}
